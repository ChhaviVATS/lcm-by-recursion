#include<stdio.h>
int lcm(int,int);
int main()
{
	int a,b,result;
	int prime[100];
	printf("enter two numbers:");
	scanf("%d %d",&a,&b);
	result=lcm(a,b);
	printf("the lcm of %d and %d is %d\n",a,b,result);
	return 0;
}
int lcm(inta,intb)
{
	static int common=1;
	if(common% inta==0 && common% intb==0)
	{
		return common;
		
	}
	common++;
	lcm(inta,intb);
	return common;
}
